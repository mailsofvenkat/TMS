<?php

namespace MS\MsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * User
 */
class User
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $user_type;

    /**
     * @var string
     */
    private $nick_name;

    /**
     * @var string
     */
    private $first_name;

    /**
     * @var string
     */
    private $last_name;

    /**
     * @var string
     */
    private $email;

    /**
     * @var string
     */
    private $password;

    /**
     * @var string
     */
    private $salt;

    /**
     * @var string
     */
    private $status;

    /**
     * @var integer
     */
    private $id_chat_status;

    /**
     * @var string
     */
    private $sex;

    /**
     * @var string
     */
    private $descriptionMe;

    /**
     * @var string
     */
    private $country;

    /**
     * @var string
     */
    private $city;

    /**
     * @var \DateTime
     */
    private $date_of_birth;

    /**
     * @var \DateTime
     */
    private $date_reg;

    /**
     * @var \DateTime
     */
    private $date_last_login;

    /**
     * @var string
     */
    private $type;

    /**
     * @var string
     */
    private $facebook_url;

    /**
     * @var string
     */
    private $twitter_url;

    /**
     * @var string
     */
    private $youtube_url;

    /**
     * @var string
     */
