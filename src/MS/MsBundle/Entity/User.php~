     * Set descriptionMe
     *
     * @param string $descriptionMe
     * @return User
     */
    public function setDescriptionMe($descriptionMe)
    {
        $this->descriptionMe = $descriptionMe;

        return $this;
    }

    /**
     * Get descriptionMe
     *
     * @return string 
     */
    public function getDescriptionMe()
    {
        return $this->descriptionMe;
    }

    /**
     * Set country
     *
     * @param string $country
     * @return User
     */
    public function setCountry($country)
    {
        $this->country = $country;

        return $this;
    }

    /**
     * Get country
     *
     * @return string 
     */
    public function getCountry()
